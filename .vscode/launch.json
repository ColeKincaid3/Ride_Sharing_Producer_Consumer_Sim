    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [ "-n 150", "-h 5", "-a 15", "-c 35", "-f 20" ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "enviornment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "C/C++: clang++ build active file"
        }
    ]
}